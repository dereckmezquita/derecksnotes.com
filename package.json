{
  "name": "derecksnotes.com",
  "version": "3.0.0",
  "homepage": "https://github.com/dereckmezquita/derecksnotes.com#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dereckmezquita/derecksnotes.com.git"
  },
  "bugs": {
    "url": "https://github.com/dereckmezquita/derecksnotes.com/issues"
  },
  "author": "Dereck Mezquita",
  "license": "ISC",
  "scripts": {
    "build-client": "cd client && npm run build",
    "build-server": "tsc --project server/tsconfig.json",
    "restart-server-prod": "sudo systemctl restart derecksnotes-server",
    "restart-server-dev": "sudo systemctl restart next-derecksnotes-server",
    "build": "npm run build-server && npm run build-client",
    "server-logs-dev": "sudo journalctl -f -u next-derecksnotes-server",
    "server-logs-prod": "sudo journalctl -f -u derecksnotes-server",
    "prepare": "husky install",
    "format": "prettier --write ."
  },
  "prettier": {
    "semi": true,
    "trailingComma": "none",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "overrides": [
      {
        "files": "*.yml",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "colors": "^1.4.0",
    "connect-redis": "^6.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "country-flag-icons": "^1.5.5",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.1",
    "date-format": "^4.0.13",
    "ejs": "^3.1.8",
    "ejs-easy-loader": "^0.1.4",
    "ejs-loader": "^0.5.0",
    "ejs-webpack-loader": "^2.2.2",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "file-loader": "^6.2.0",
    "form-data": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "merge": "^2.1.1",
    "mini-css-extract-plugin": "^2.6.1",
    "mongodb": "^4.8.1",
    "mongoose": "^7.5.3",
    "node-html-markdown": "^1.3.0",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.4",
    "sanitize-html": "^2.10.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.9",
    "ts-loader": "^9.3.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "validator": "^13.7.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-fix-style-only-entries": "^0.6.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.23",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "@types/sanitize-html": "^2.11.0",
    "@types/styled-components": "^5.1.34",
    "@types/validator": "^13.11.9",
    "husky": "^8.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
